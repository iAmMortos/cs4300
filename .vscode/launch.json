  "version": "0.2.0",
  "configurations": [
    {
      "name": "OOP Xml Parse",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/project/oop/oop_xml_parse.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/project/oop"
    },
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "cwd": "${fileDirname}"
    },
  ]
}